{
	last_obj.parentNode.style.background = "#2C3B41";
}
last_obj = obj;
obj.parentNode.style.background = "rgba(0, 184, 255, 0.48)"; //令当前标签高亮
}
//当然也可以用Jquery的$("a").removeCss() 和addCss()来实现
</script> 
